<% form_for :user, :url => { :action => :authenticate } do |form| %>
<p>
<%= label :user, :username, "UserName:" %>
<%= form.text_field :username, :size => 30 %>
</p>
<p>
<%= label :user, :password, "Password:" %>
<%= form.password_field :password, :size => 30 %>
</p>
<%= submit_tag "login" , :class => "submit" %>
<% end %>
